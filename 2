C=randi([1,100],10,10); %create new matrix
A= zeros(10, 10); 
for i=1:10
    for j=1:10
        if C(i,j)>=1 & C(i,j)<=33
            A(i,j)=1;
        else if C(i,j)>=34 & C(i,j)<=66
                A(i,j)=2;
        else
            A(i,j)=3;
        end
        end
    end
end
imagesc(A); 
colormap([0 0 1; 0 1 0; 1 0 0]); % for blue ,red,green colour
colorbar;
